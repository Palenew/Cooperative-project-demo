    float:left;
    height:32px;
    line-height: 32px;
    /*overflow: hidden;*/
}
.header-bot .header-bot-right{
    position: fixed;
   left:36%;
  top:50px;
    z-index: 1;
}
.header-bot .header-bot-right .ul-lists a{
    display:inline-block ;
    padding:0 24px;
    color:#333333;
    font-size: 14px;
}
.header-bot .header-bot-right .ul-lists a:hover{
    /*border-bottom: 2px solid #BF2400;*/
    color:#BF2400;
}
.header-bot .header-bot-right .ul-lists li{
    position: relative;
}
.header-bot .header-bot-right .ul-lists .ul-lists-pubic{
    /*width:70px;*/
    height:2px;
    /*background-color: red;*/
    position: absolute;
    left:18px;
}
.header-bot .header-bot-right .ul-lists .ul-lists-show{
    background-color: #666;

    width:70px;
    font-size: 14px;
    display: none;
    position: absolute;
    left:18px;
    top:35px;
    text-align: center;
    z-index:1;
}
.header-bot .header-bot-right .ul-lists .ul-lists-show a{
    padding:0;
    display: block;
    border-bottom: 1px solid gray;
    color:white;
}
