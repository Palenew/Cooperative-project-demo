.map-box .map-con {
    position: absolute;
    cursor: pointer;
    width: 200px;
    height: 120px;
    left: 50%;
    top: 50%;
    margin: -60px 0 0 -100px;
    z-index: 2;
}

.map-box .map-con i {
    display: block;
    background: url(../images/pos.png) no-repeat;
    width: 43px;
    height: 56px;
    margin: 0 auto 20px;
    animation: myanimate 0.5s infinite alternate;
    -webkit-animation: myanimate 0.5s infinite alternate;
}

.map-box .map-con a {
    display: block;
    border: 2px solid #fff;
    padding: 2px 0;
    text-align: center;
    font-size: 1.29em;
    color: #fff;
    border-radius: 5px;
}

@keyframes myanimate {
    0% {
        transform: translateY(0) scale(1, 1);
    }
    100% {
        transform: translateY(15px) scale(0.9, 0.9);
    }
}

@-webkit-keyframes myanimate {
    0% {
        transform: translateY(0) scale(1, 1);
    }
    100% {
        transform: translateY(15px) scale(0.9, 0.9);
    }
}

