.window {
    width: 300px;
    height: 300px;
    position: fixed;
    margin :100px auto;
    top: 350px;
    left: 1300px;

}

.window li{
    width: 45px;
    height: 100px;
    border-radius: 50%;
    font-size: 20px;
    text-align: center;
    line-height: 100px;
    background-color: rgba(204, 204, 204,0.5);

    /*opacity: 0.5;*/
    box-shadow: 0 3px 0 yellowgreen ;
    color: #fff;
    cursor: pointer;

    position: absolute;
    left: 0;
    top: 0;
    transition: all 1s;

    transform-origin: left top;
    transform-origin: right top;
}
.window:hover{
    /*left: 1000px;*/
}
.window:hover li:nth-child(1) {
    transform: rotate(60deg);
}

.window:hover li:nth-child(2) {
    transform: rotate(120deg);
}

.window:hover li:nth-child(3) {
    transform: rotate(180deg);
}

.window:hover li:nth-child(4) {
    transform: rotate(240deg);
}

.window:hover li:nth-child(5) {
    transform: rotate(300deg);
}

.box:hover li:nth-child(6) {
    transform: rotate(360deg);
}

