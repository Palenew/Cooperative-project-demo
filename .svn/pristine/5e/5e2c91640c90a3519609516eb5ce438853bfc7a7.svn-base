.box{
    width: 1350px;
    height: 450px;
    margin: 100px auto;
    position: relative;
    margin-top: 16px;
    display: none;

}
.box ul{
    position: absolute;
    top: 0;
    left: 0;
}
.box ul li img{
    width: 1350px;
    height: 450px;
}
.box ul li{
    float: left;
    position: absolute;
    top: 0;
    left: 0;
    display: none;
}
.box ol{
    position: absolute;
    right: 15px;
    bottom: 15px;
    z-index: 1000;
}
.box ol li{
    float: left;
    width: 20px;
    height: 20px;
    background: #7C7575;
    border: 1px solid #3A3636;
    background: rgba(0, 0, 0, 0.3);
    margin: 2px 5px;
    cursor: pointer;
    text-align: center;
    line-height: 20px;
}
.box ol li.current{
    background: rgba(0, 0, 0, 0.6);
    color: #0CF344;
}
.left,.right{
    width: 41px;
    height: 420px;
    position: absolute;
    top: 0;
    background: url(../img1/arr.png) no-repeat left center;
    z-index: 999;
}
.left{
    background-position: -84px center;
    left: 0;
}
.right{
    right: 0;
    background-position: -125px center;
}
.left:hover{
    background-position: left center;
}
.right:hover{
    background-position: -42px center;
}



