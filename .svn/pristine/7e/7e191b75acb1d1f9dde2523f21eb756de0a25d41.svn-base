
/*******************************
	-------[=banner]-------
********************************/
#banner{}
#banner .slider{height: 100%;position: relative;
	background-repeat: no-repeat;
	background-position: center center;
	-o-background-size: cover;
	-ms-background-size: cover;
	-moz-background-size: cover;
	-webkit-background-size: cover;
	background-size: cover;
}
#banner .txt{position: absolute;left: 0;right: 0;top: 25%;bottom: 15%;text-align: center;}
#banner .txt h1{}
#banner .txt h1 img{
	max-width: 60%;
	-o-transition:.5s;
	-ms-transition:.5s;
	-moz-transition:.5s;
	-webkit-transition:.5s;
	transition:.5s;
}
#banner .txt h1 img:hover{
	-o-transform:scale(1.1);
	-ms-transform:scale(1.1);
	-moz-transform:scale(1.1);
	-webkit-transform:scale(1.1);
	transform:scale(1.1);
}
#banner .txt .btn{position: absolute;bottom: 0;left: 0;right: 0;}

.g-top{color: #434343;line-height: 35px;text-align: center;overflow: hidden;
	position: absolute;left: 0;top: 85px;right: 0;z-index: 2;
}
.g-top ul{text-align: left;}
.g-top li{float: left;width: 50%;}
.g-top li.on,.g-top li.on a{color: #e42338;}
.g-top li:first-child{text-align: right;}
.g-top h3{font-size: 24px;position: relative;padding: 0 15px;cursor: pointer;display: inline-block;}
.g-top h3 em{display: block;font-weight: bold;position: relative;white-space: nowrap;word-wrap: normal;text-overflow: ellipsis;}
.g-top h3:after,.g-top h3 em:after{
	content: '';position: absolute;left: 100%;height: 28px;
	border-right: 2px solid #434343;
}
.g-top li:last-child h3:after,.g-top li:last-child h3 em:after{display: none;}
.g-top h3:after{bottom: 5px;}
.g-top h3 em:after{top: 50%;margin-left: 15px;margin-top: -14px;}

.g-con{padding-top: 90px;position: relative;z-index: 1;font-size: 14px;}

.section .canvas{z-index: 0;}

